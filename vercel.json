{
    "version": 2,
    "builds": [
      { "src": "server/app.py", "use": "@vercel/python" },
      { "src": "client/dist/**", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/static/(.*)", "dest": "/client/dist/static/$1" },
      { "src": "/(.*)", "dest": "/server/app.py" }
    ]
  }
  